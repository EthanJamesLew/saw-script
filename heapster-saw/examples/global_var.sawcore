module GlobalVar where

import Prelude;

acquireLockM : Vec 64 Bool ->
               SpecM Void VoidEvRetType emptyFunStack (Vec 64 Bool * Vec 64 Bool);
acquireLockM u =
  retS Void VoidEvRetType emptyFunStack (Vec 64 Bool * Vec 64 Bool) (u,u);

releaseLockM : Vec 64 Bool -> Vec 64 Bool ->
               SpecM Void VoidEvRetType emptyFunStack (Vec 64 Bool);
releaseLockM u new_u =
  retS Void VoidEvRetType emptyFunStack (Vec 64 Bool) new_u;
